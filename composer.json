		"govtnz/collectiontools": "dev-master",
		"unclecheese/display-logic": "*@stable"
 	},
	"authors": [
 		{
 			"name": "wakes",
 			"email": "wakes@github.com",
 			"homepage": "https://github.com/wakes/",
